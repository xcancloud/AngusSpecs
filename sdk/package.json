{
  "name": "@angus/sdk",
  "version": "1.0.0",
  "description": "Core Type Definitions for Angus ecosystem - Provides unified type contracts and data structure definitions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "dev": "npm run build:watch"
  },
  "keywords": [
    "typescript",
    "types",
    "angus",
    "type-definitions",
    "data-structures",
    "contracts"
  ],
  "author": "Angus Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xcancloud/AngusSpecs.git",
    "directory": "types"
  },
  "bugs": {
    "url": "https://github.com/xcancloud/AngusSpecs/issues"
  },
  "homepage": "https://github.com/xcancloud/AngusSpecs#readme",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
